        <Button Grid.Row="4" Grid.Column="1" Background="Green" Margin="10" Name="Table22" Content="22" FontSize="36"></Button>
        <Button Grid.Row="4" Grid.Column="2" Background="Green" Margin="10" Name="Table23" Content="23" FontSize="36"></Button>
        <Button Grid.Row="4" Grid.Column="3" Background="Green" Margin="10" Name="Table24" Content="24" FontSize="36"></Button>
